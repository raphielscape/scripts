# Device Environment container

mido() {
	DEVICE=mido
	DEFCONFIG="raph_defconfig"
	SDM845=false
}

beryllium() {
	DEVICE=beryllium
	DEFCONFIG="raph_beryllium_defconfig"
	SDM845=true
}

dipper() {
	DEVICE=dipper
	DEFCONFIG="raph_dipper_defconfig"
	SDM845=true
}

export DEFCONFIG DEVICE SDM845